<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="411.89dp"
    android:layout_height="162.16dp"
    android:background="@color/dialog_bg"
    android:gravity="center_vertical"
    android:orientation="vertical">

    <android.support.v7.widget.RecyclerView
        android:id="@+id/rv_disk_list"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="6.49dp" />

    <TextView
        android:id="@+id/tv_cancel"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="31.08dp"
        android:layout_marginBottom="21.62dp"
        android:text="@string/cancel"
        android:textColor="@color/text_color_highlight"
        android:textSize="@dimen/font_15" />
</RelativeLayout>
