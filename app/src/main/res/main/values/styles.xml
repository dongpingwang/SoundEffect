<resources>

    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowNoTitle">true</item>
        <item name="android:windowBackground">@color/window_background</item>
    </style>


    <style name="eq_gain_left_title_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_color_white</item>
        <item name="android:textSize">@dimen/font_13</item>
        <item name="android:layout_gravity">end</item>
    </style>

    <style name="dialog_action_tv">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">@color/text_color_white</item>
        <item name="android:textSize">@dimen/font_15</item>
        <item name="android:background">@drawable/comm_ripple_rectangle_dialog</item>
        <item name="android:padding">5dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:layout_marginBottom">21.62dp</item>
        <item name="android:layout_alignParentBottom">true</item>
    </style>
</resources>